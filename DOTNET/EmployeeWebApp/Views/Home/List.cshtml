@model List< EmployeeWebApp.Models.Employee>
<h1>Employee List</h1>
<table align="center" border="1" cellspacing="200">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Salary</th>
            <th>City</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employ in Model)
        {
            <tr>
                <td>@employ.id</td>
                <td>@employ.firstName</td>
                <td>@employ.lastName</td>
                <td>@employ.salary</td>
                <td>@employ.city</td>
                <td>@employ.email</td>
            </tr>
        }
    </tbody>
</table>

<a href="Login">To Login Page</a>
